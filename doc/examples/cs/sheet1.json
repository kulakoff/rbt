        "sheet": "_cf_sheet",
        "date": "_cf_sheet_date",
        "col": "_cf_sheet_col",
        "row": "_cf_sheet_cell",
        "cells": "_cf_sheet_cells",
        "assigned": "_cf_installers"
    },
    "issuesQuery": {
        "query": {
            "$and": [
                {
                    "_cf_sheet": "%%sheet"
                },
                {
                    "_cf_sheet_date": "%%date"
                },
                {
                    "workflow": {
                        "$in": [
                            "lanta"
                        ]
                    }
                }
            ]
        },
        "fields": [
            "_cf_sheet_col",
            "_cf_sheet_cell",
            "_cf_sheet_cells",
            "_cf_installers",
            "_cf_install_done",
            "status"
        ],
        "limit": 500,
        "preprocess": {
            "%%sheet": "МКД",
            "%%date": "2023-07-14"
        },
        "project": "RTL"
    },
